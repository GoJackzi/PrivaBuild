# Alchemy RPC URL for Sepolia testnet
# Get your API key at https://alchemy.com
ALCHEMY_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY

# Your wallet private key for deploying contracts
# IMPORTANT: Never commit your actual private key! This is just an example.
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Pinata JWT token for IPFS uploads
# Sign up at https://pinata.cloud and create a JWT token
PINATA_JWT=your_pinata_jwt_token_here

# Etherscan API key for contract verification
# Get your API key at https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: Pinata Gateway URL (defaults to gateway.pinata.cloud if not set)
NEXT_PUBLIC_GATEWAY_URL=gateway.pinata.cloud
